{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "browsers": ["> 3%", "IE 11"],
                    "node": 10
                },
                "corejs": 3,
                "exclude": [
                    "transform-typeof-symbol",
                    "es.function.name",
                    "es.string.link",
                    "es.number.constructor",
                    "es.string.split",
                    "es.string.replace",
                    "es.array.concat",
                    "es.array.filter",
                    "es.array.find",
                    "es.array.for-each",
                    "es.array.index-of",
                    "es.array.join",
                    "es.array.map",
                    "es.array.slice",
                    "es.array.splice",
                    "web.dom-collections.for-each",
                    "es.regexp.exec"
                ],
                "useBuiltIns": "usage"
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-proposal-nullish-coalescing-operator",
        "@babel/plugin-proposal-optional-chaining",
        "@babel/plugin-proposal-class-properties"
    ]
}
